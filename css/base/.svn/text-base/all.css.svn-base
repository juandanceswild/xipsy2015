/* wrapper styles */
.wrapper {
  height: 100%;
  white-space: nowrap; }
  .wrapper .form-box {
    display: inline-block;
    white-space: normal;
    vertical-align: middle; }
  .wrapper:before {
    content: '';
    vertical-align: middle;
    display: inline-block;
    height: 100%;
    width: 1px;
    overflow: hidden;
    margin: 0 0 0 -5px; }

