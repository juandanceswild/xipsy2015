/* styles for flip animation */
.flip-mask {
  position: relative;
  -webkit-transform-style: preserve-3d;
  -moz-transform-style: preserve-3d;
  -o-transform-style: preserve-3d;
  transform-style: preserve-3d;
  -webkit-transform: perspective(1000);
  -moz-transform: perspective(1000);
  -o-transform: perspective(1000);
  transform: perspective(1000);
  -webkit-transform: perspective(1000px);
  -moz-transform: perspective(1000px);
  -o-transform: perspective(1000px);
  transform: perspective(1000px); }
  .flip-mask .flip-slide {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    -webkit-transform: rotateY(0deg);
    -ms-transform: rotateY(0deg);
    -o-transform: rotateY(0deg);
    transform: rotateY(0deg);
    -webkit-backface-visibility: hidden;
    -ms-backface-visibility: hidden;
    -o-backface-visibility: hidden;
    backface-visibility: hidden;
    -webkit-transition: 0.6s;
    -moz-transition: 0.6s;
    -ms-transition: 0.6s;
    -o-transition: 0.6s;
    transition: 0.6s;
    -webkit-transform-style: preserve-3d;
    -moz-transform-style: preserve-3d;
    -o-transform-style: preserve-3d;
    transform-style: preserve-3d;
    /* flip front styles */
    /* flip back styles */ }
    .flip-mask .flip-slide.front-side {
      background: #0a0045;
      -webkit-transform: rotateY(0deg);
      -ms-transform: rotateY(0deg);
      -o-transform: rotateY(0deg);
      transform: rotateY(0deg);
      z-index: 2; }
    .flip-mask .flip-slide.back-side {
      color: #666;
      -webkit-transform: rotateY(-180deg);
      -ms-transform: rotateY(-180deg);
      -o-transform: rotateY(-180deg);
      transform: rotateY(-180deg); }
  .flip-active .flip-mask .front-side {
    -webkit-transform: rotateY(180deg);
    -ms-transform: rotateY(180deg);
    -o-transform: rotateY(180deg);
    transform: rotateY(180deg); }
  .flip-active .flip-mask .back-side {
    -webkit-transform: rotateY(0deg);
    -ms-transform: rotateY(0deg);
    -o-transform: rotateY(0deg);
    transform: rotateY(0deg); }
  .flip-mask .flipper {
    -webkit-transform-style: preserve-3d;
    -moz-transform-style: preserve-3d;
    -o-transform-style: preserve-3d;
    transform-style: preserve-3d;
    -webkit-perspective: -webkit-transform perspective(1000px);
    -moz-perspective: -moz-transform perspective(1000px);
    -o-perspective: -o-transform perspective(1000px);
    perspective: transform perspective(1000px);
    -webkit-transition: 0.6s;
    -moz-transition: 0.6s;
    -ms-transition: 0.6s;
    -o-transition: 0.6s;
    transition: 0.6s;
    position: relative;
    overflow: hidden;
    min-height: 65px; }
    @media only screen and (max-width: 1025px) {
      .flip-mask .flipper {
        min-height: 210px; } }
    @media only screen and (max-width: 767px) {
      .flip-mask .flipper {
        min-height: 270px; } }

/* login link styles */
span.link-request {
  display: none; }
  .flip-active span.link-request {
    display: inline; }

a.link-request {
  position: relative;
  text-decoration: none;
  overflow: hidden;
  position: relative; }
  a.link-request:after {
    position: absolute;
    content: "";
    top: auto;
    bottom: 0;
    left: 0;
    right: 0; }
  a.link-request:after {
    height: 1px;
    background: #fff; }
  a.link-request:hover:after {
    background: none; }
  .flip-active a.link-request {
    display: none; }

.flip-active span.link-login {
  display: none; }

a.link-login {
  display: none;
  text-decoration: none;
  position: relative; }
  a.link-login:after {
    position: absolute;
    content: "";
    top: auto;
    bottom: 0;
    left: 0;
    right: 0; }
  a.link-login:after {
    height: 1px;
    background: #fff; }
  a.link-login:hover:after {
    background: none; }
  .flip-active a.link-login {
    display: inline; }
