/* body styles */
input[type="search"] {
  -webkit-appearance: none; }

input[type="search"]::-webkit-search-cancel-button {
  -webkit-appearance: none; }

input[type="search"]::-webkit-search-decoration {
  display: none; }

input[type="email"] {
  -webkit-appearance: none; }

input[type="email"]::-webkit-search-cancel-button {
  -webkit-appearance: none; }

input[type="email"]::-webkit-search-decoration {
